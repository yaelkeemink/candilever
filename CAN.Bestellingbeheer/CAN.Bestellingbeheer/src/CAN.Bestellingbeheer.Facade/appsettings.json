{
 "Serilog": {
    "WriteTo": [
      {
        "Name": "RollingFile",
        "MinimumLevel": "Debug",
        "Args": {
          "pathFormat": "/var/log/Kantilever/BestellingbeheerLogging/log-{Date}.txt"
        }
      }
    ],  
    "Enrich": ["FromLogContext", "WithMachineName"],
